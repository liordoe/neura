{
  "name": "diplom",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:app": "webpack --config webpack-app.config.js",
    "build:front": "webpack --config webpack-front.config.js",
    "build": "npm run build:app && npm run build:front",
    "watch:app": "webpack --config webpack-app.config.js --watch",
    "watch:front": "webpack --config webpack-front.config.js --watch",
    "start": "npm-run-all build:app build:front && node dist/app.js",
    "start:nodemon": "nodemon --delay 1 dist/app.js localhost 8080 --watch dist/",
    "dev": "npm-run-all --parallel start:nodemon watch:app watch:front",
    "dev:app": "npm-run-all --parallel start:nodemon watch:app",
    "dev:front": "npm-run-all --parallel start:nodemon watch:front",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
